{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Mango Army Launcher",
  "version": "1.0.0",
  "identifier": "com.mangoarmy.launcher",
  "build": {
    "frontendDist": "../dist-renderer",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Mango Army Launcher",
        "width": 1100,
        "height": 700,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "externalBin": [
      "bin/launcher"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "CONTENT_OF_YOUR_PUBLIC_KEY_HERE",
      "endpoints": [
        "https://github.com/Zer0Dev-exe/LauncherCustom/releases/latest/download/latest.json"
      ]
    }
  }
}